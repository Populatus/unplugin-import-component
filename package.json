    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@populatus/eslint-config": "^1.1.0",
    "@types/node": "^18.7.18",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11"
  },
  "dependencies": {
    "vite": "^4.1.0",
    "@types/fs-extra": "^11.0.1",
